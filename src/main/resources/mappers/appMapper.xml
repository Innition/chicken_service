<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.lazylhxzzy.save_upload.mapper.AppMapper">
    <select id="findLatestApp" resultType="cn.lazylhxzzy.save_upload.pojo.App">
            SELECT * FROM saves.app ORDER BY id DESC LIMIT 1
    </select>
    <select id="findByVersion" resultType="cn.lazylhxzzy.save_upload.pojo.App">
            SELECT * FROM saves.app WHERE version = #{version}
    </select>
    <insert id="insertApp" parameterType="cn.lazylhxzzy.save_upload.pojo.App">
        INSERT INTO saves.app (id, name, version, committime, url, description) VALUES (#{id}, #{name}, #{version}, #{commitTime}, #{url}, #{description})
    </insert>
    <select id="findAll" resultType="cn.lazylhxzzy.save_upload.pojo.App">
        SELECT * FROM saves.app
    </select>
</mapper>